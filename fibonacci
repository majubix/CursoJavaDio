public static void main(String[] args) {
		int n = Integer.parseInt(JOptionPane.showInputDialog("Digite um n√∫mero: "));
		System.out.println(fib(n));
		
		
		
	}

	private static int fib(int n) {
		if(n<1)
			return 0;

		int termo1 = 0;
		int termo2 = 1;
		for (int i = 2; i <= n; i++) {
			int temp = termo2;
			termo2 = termo1 +termo2;
			termo1 = temp;
		}
		return termo2;
	}

}